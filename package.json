{
	"name": "Wiki-ws",
	"displayName": "Wiki-ws",
	"description": "You can curd you blog from Wiki.js use this extension.",
	"version": "1.0.2",
	"publisher": "WhenSunset",
	"icon": "resources/icon.png",
	"repository": {
		"url": "",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:wiki",
		"onCommand:wiki.uploadAssetToWiki",
		"onCommand:wiki.deleteFileFromWiki",
		"onCommand:wiki.uploadFilesInDirToWiki",
		"onCommand:wiki.uploadFileToWiki",
		"onCommand:wiki.initWiki",
		"onCommand:wiki.resetWiki",
		"onCommand:wiki.searchInWiki"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "wiki.uploadAssetToWiki",
				"title": "wiUploadAssetToWiki(上传资源文件到Wiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.deleteFileFromWiki",
				"title": "wiDeleteFileFromWiki(从Wiki删除该文件)",
				"category": "wiki"
			},
			{
				"command": "wiki.uploadFilesInDirToWiki",
				"title": "wiUploadFilesInDirToWiki(上传目录中所有的MD文件到Wiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.uploadFileToWiki",
				"title": "wiUploadFileToWiki(上传MD文件到Wiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.initWiki",
				"title": "wiInitWiki(初始化Wiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.resetWiki",
				"title": "wiResetWiki(删除所有本地缓存Wiki文件)",
				"category": "wiki"
			},
			{
				"command": "wiki.searchInWiki",
				"title": "wiSearchInWiki(搜索Wiki中的文件)",
				"category": "wiki"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "wiki.uploadFilesInDirToWiki",
					"when": "resourceScheme != wiki && explorerResourceIsFolder"
				},
				{
					"command": "wiki.uploadFileToWiki",
					"when": "resourceExtname == .md"
				},
				{
					"command": "wiki.deleteFileFromWiki",
					"when": "resourceExtname == .md && resourceScheme == wiki"
				},
				{
					"command": "wiki.uploadAssetToWiki",
					"when": "resourceExtname != .md && resourceScheme != wiki && !explorerResourceIsFolder"
				}
			],
			"commandPalette": [
				{
					"command": "wiki.resetWiki",
					"when": "workbenchState == workspace"
				},
				{
					"command": "wiki.searchInWiki",
					"when": "workbenchState == workspace"
				},
				{
					"command": "wiki.initWiki",
					"when": "workbenchState != workspace"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/ini": "^1.3.30",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"axios": "^0.22.0",
		"form-data": "^4.0.0",
		"graphql": "^15.6.1",
		"graphql-request": "^3.5.0",
		"ini": "^2.0.0"
	}
}
