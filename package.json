{
	"name": "Wiki-ws",
	"displayName": "Wiki-ws",
	"description": "You can curd you blog from Wiki.js use this extension.",
	"version": "1.1.5",
	"publisher": "WhenSunset",
	"icon": "resources/icon.png",
	"repository": {
		"url": "",
		"type": "git"
	},
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onFileSystem:wiki",
		"onCommand:wiki.uploadAssetToWiki",
		"onCommand:wiki.deleteFileFromWiki",
		"onCommand:wiki.uploadFilesInDirToWiki",
		"onCommand:wiki.uploadFileToWiki",
		"onCommand:wiki.initWiki",
		"onCommand:wiki.initWikiWindows",
		"onCommand:wiki.resetWiki",
		"onCommand:wiki.searchInWiki",
		"onCommand:wiki.deleteDirFileFromWiki"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "wiki.uploadAssetToWiki",
				"title": "上传资源文件到Wiki(UploadAssetToWiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.deleteFileFromWiki",
				"title": "从Wiki删除该文件(DeleteFileFromWiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.deleteDirFileFromWiki",
				"title": "从Wiki删除该目录下的所有MD文件(DeleteDirFileFromWiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.uploadFilesInDirToWiki",
				"title": "上传目录中所有的MD文件到Wiki(UploadFilesInDirToWiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.uploadFileToWiki",
				"title": "上传文件到Wiki(UploadFileToWiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.initWiki",
				"title": "初始化Wiki(InitWiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.initWikiWindows",
				"title": "初始化Wiki(InitWikiWindows)",
				"category": "wiki"
			},
			{
				"command": "wiki.searchInWiki",
				"title": "在Wiki中搜索文件(SearchInWiki)",
				"category": "wiki"
			},
			{
				"command": "wiki.fetchAllFileFromWikiInDir",
				"title": "从Wiki中加载当前目录下所有文件(FetchAllFileFromWikiInDir)",
				"category": "wiki"
			},
			{
				"command": "wiki.fileStatus",
				"title": "当前文件的状态(FileStatus)",
				"category": "wiki"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "wiki.fileStatus",
					"when": "resourceScheme == wiki"
				}
			],
			"explorer/context": [
				{
					"command": "wiki.uploadFilesInDirToWiki",
					"when": "explorerResourceIsFolder"
				},
				{
					"command": "wiki.uploadFileToWiki",
					"when": "resourceExtname == .md"
				},
				{
					"command": "wiki.deleteFileFromWiki",
					"when": "resourceExtname == .md && resourceScheme == wiki"
				},
				{
					"command": "wiki.deleteDirFileFromWiki",
					"when": "explorerResourceIsFolder && resourceScheme == wiki"
				},
				{
					"command": "wiki.fetchAllFileFromWikiInDir",
					"when": "explorerResourceIsFolder && resourceScheme == wiki"
				},
				{
					"command": "wiki.uploadAssetToWiki",
					"when": "resourceExtname != .md && resourceScheme != wiki && !explorerResourceIsFolder"
				}
			],
			"commandPalette": [
				{
					"command": "wiki.searchInWiki",
					"when": "workbenchState == workspace"
				},
				{
					"command": "wiki.initWiki",
					"when": "workbenchState != workspace && !isWindows"
				},
				{
					"command": "wiki.initWikiWindows",
					"when": "workbenchState != workspace && isWindows"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/ini": "^1.3.30",
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2"
	},
	"dependencies": {
		"axios": "^0.22.0",
		"form-data": "^4.0.0",
		"graphql": "^15.6.1",
		"graphql-request": "^3.5.0",
		"ini": "^2.0.0",
		"node-stream-zip": "^1.15.0",
		"opn": "^6.0.0"
	}
}
